RANGE OF Ime IS Relacija

1. 	RANGE OF PX IS PREDMET
	PX.SIFRA, PX.NAZIV

2.	RANGE OF PX IS PREDMET
	PX.ID_PREDMETA, PX.SIFRA, PX.NAZIV, PX.BODOVI	(PX.*)
	WHERE PX.BODOVI = 6

3.	RANGE OF IX IS ISPIT
	RANGE OF PX IS PREDMET

	IX.INDEKS, IX.OCENA, PX.NAZIV
	WHERE IX.ID_PREDMETA = PX.ID_PREDMETA

4.	RANGE PX, PY IS PREDMET
	PX.SIFRA, PY.SIFRA, PX.NAZIV, PY.NAZIV
	WHERE PX.BODOVI = PY.BODOVI AND PX.ID_PREDMETA < PY.ID_REDMETA	// '<>' je satro '!='

WHERE EXISTS Var(formula)

	// ako zadovoljava uslov vraca tacno
	// NOT EXISTS Var(NOT formula)

5.	RANGE OF IX IS ISPIT
	RANGE OF PX IS PREDMET
	PX.SIFRA, PX.NAZIV
	WHERE EXISTS IX(	IX.OCENA > 5 AND IX.INDEKS = 20140026 // IX se moze ponovo koristiti
						AND PX.ID_PREDMETA = IX.ID_PREDMETA)
	// AND IX.BODOVI = 70 ne mere
	// mozemo da koristimo samo promenljive nevedene u prvoj liniji

WHERE FORALL Var(formula)	// izdvajamo sve koji zadovoljavaju uslov

6.	RANGE OF IX IS ISPIT
	RANGE OF PX IS PRDMET
	RANGE OF DX IS DOSIJE
	DX.INDEKS
	WHERE FORALL PX(EXISTS IX(IX.ID_PREMETA = PX.ID_PREDMETA AND IX.INDEKS = DX.INDEKS))

	// AND EXISTS IX(IX.OCENA = 10 AND IX.INDEKS = DX.INDEKS)
	// Ako necemo FORALL

	DX.INDEKS
	WHERE NOT EXISTS PX(NOT EXISTS IX(IX.ID_REDMET = PX.ID_PREDMETA AND IX.INDEKS = DX.INDEKS))

7.	RANGE OF IX IS ISPIT
	RANGE OF PX IS PREDMET
	RANGE OF DX IS DOSIJE
	PX.NAZIV
	WHERE PX.BODOVI = 6 AND EXISTS IX(	IX.ID_PREDMETA = PX.ID_PREDMETA
										AND EXISTS DX(	DX.INDEKS = IX.INDEKS
														AND DX.PREZIME = "Vukovic"))
// IF uslov1 THEN uslov2

8.	RANGE OF PX IS PREDMET
	RANGE OF IX, IY IS ISPIT
	RANGE OF DX IS DOSIJE
	DX.INDEKS
	WHERE FORALL PX(IF EXISTS IX(	IX.ID_PREDMETA = PX.ID_PREDMETA
									AND IX.OCENA > 5 AND IX.INDEKS = 20140025)
					THEN EXISTS IY(IY.ID_PREDMETA = PX.ID_PREDMETA
					AND IY.OCENA > 5 AND IY.INDEKS = DX.INDEKS))

	// -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
	// p => q
	// !p or q
	// NOT p OR q

	//FORALL PX(IF P THEN Q)
	//FORALL PX(NOT P OR Q)
	//NOT EXISTS PX(NOT(NOT P OR Q))
	//NOT EXISTS PX(P AND NOT Q)
	// -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-

8a.	RANGE OF PX IS PREDMET
	RANGE OF IX, IY IS ISPIT
	RANGE OF DX IS DOSIJE
	DX.INDEKS
	WHERE NOT EXISTS PX(EXIST IX(	IX.ID_PREDMETA = PX.ID_PREDMETA
									AND IX.OCENA > 5 AND IX.INDEKS = 20140025)
						AND NOT EXISTS IY(IY.ID_PREDMETA = PX.ID_PREDMETA AND IY.OCENA > 5
						AND IY.INDEKS = DX.INDEKS))

8b.	RANGE OF PX IS PREDMET
	RANGE OF IX, IY IS ISPIT
	RANGE OF DX IS DOSIJE
	DX.INDEKS
	WHERE FORALL IX(IF IX.INDEKS = 20140025 AND IX.OCENA > 5
					THEN EXISTS IY(	IY.ID_PREDMETA = IX.ID_PREDMETA AND IY.OCENA > 5
									AND DX.INDEKS = IY.INDEKS))

9.	DX.INDEKS, DX.IME, DX.PREZIME
	WHERE EXISTS IX(IX.OCENA > 5 AND IX.INDEKS = DX.INDEKS AND IX.ID_PREDMETA
					AND EXISTS PX(PX.ID_PREDMETA = IX.ID_PREDMETA AND PX.NAZIV = "p1")
					AND NOT EXISTS IY(	IY.OCENA > 5 AND IY.INDEKS = DX.INDEKS
										AND IX.ID_PREDMETA <> IY.ID_PREDMETA))

10.
	a) algebra:

		DEFINE ALIAS P FOR PREDMET
		PREDMET[NAZIV, BODOVI]
		MINUS
		((PREDMET TIMES P)  WHERE PREDMET.BODOVI > P.BODOVI)[P.NAZIV, P.BODOVI]

	b) racun:

		WHERE FORALL PY(PY.BODOVI <= PX.BODOVI)


