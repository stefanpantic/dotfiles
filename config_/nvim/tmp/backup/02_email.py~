#! /usr/bin/env python
# -*- coding: utf-8 -*-

inicijalno = -1
stanje = inicijalno
zavrsno = 7

# Nacrtajte automat kako bi lakse razumeli elemente mape.
f = {
    (-1, "a"): 0,
    (-1, "b"): 1,
    (0, "a"): 0,
    (1, "b"): 1,
    (0, "b"): 1,
    (1, "a"): 0,
    (0, "@"): 2,
    (1, "@"): 2,
    (2, "x"): 3,
    (3, "y"): 4,
    (4, "."): 5,
    (5, "y"): 6,
    (6, "x"): 7,
}

while True:
    try:
        akcija = input()
        stanje = f[(stanje, akcija)]
    except EOFError:
        break
    except ValueError as e:
        print("Dozvoljeno je uneti samo 1 ili 0")
        print(e)
    except KeyError as e:
        print("Dozvoljeno je uneti samo 1 ili 0")
        print(e)
        break

if stanje == zavrsno:
    print("Automat prihvata rec.")
else:
    print("Automat ne prihvata rec..")
