#!/bin/sh
cat nastava.html |
grep -Eo '<td>[A-Z]{1}[A-Za-z ]+<\/td><td>[0-9]{1,3}\/[0-9]{4}<\/td>' |
sed -E 's/<td>(.*?)<\/td><td>(.*?)<\/td>/\1 \2/' |
sed -E 's/([A-Z]{1}[a-z]+) ([A-Z]{1}[a-z]+) ([0-9]{1,3})\/([0-9]{4})/\2 \1 \3 \4/' |
sed -E 's/(.*?) ([0-9]{1}) (.*?)/\1 00\2 \3/' |
sed -E 's/(.*?) ([0-9]{2}) (.*?)/\1 0\2 \3/' |
sed -E 's/(.*?) ([0-9]{3}) [0-9]{2}([0-9]{2})/\1 mi\3\2/' |
sort |
grep -n '.' |
sed -E 's/^([0-9]+):(.*?)$/\1. \2/'
