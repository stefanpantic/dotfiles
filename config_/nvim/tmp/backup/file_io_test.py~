import os
import sys
import re

in_file = open("./test_file.txt", "a")
in_file.write(str("This is a line\n"))
in_file.close()

in_file = open("./test_file.txt", "r+")

str_f = in_file.readline()
while "" != str_f:
    if re.match('^[a-z]', str_f):
        print(str_f, end="")
    str_f = in_file.readline()
