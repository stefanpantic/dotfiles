#!/bin/python

import sys

# expression: a*|bab*
current_state = 0
end_state = (1, 3, 4)
transition_map = {
        (0, 'a') : 1,
        (0, 'b') : 2,
        (1, 'a') : 1,
        (1, 'b') : 3,
        (2, 'a') : 4,
        (3, 'b') : 3,
        (4, 'b') : 3,
}

character = ''
while True:
    try:
        character = input()
        current_state = transition_map[(current_state, character)]
    except KeyError:
        print("invalid expression...")
        sys.exit(1)
    except EOFError:
        break

if current_state in end_state:
    print("valid expression...")
else:
    print("invalid expression...")
    sys.exit(1)
