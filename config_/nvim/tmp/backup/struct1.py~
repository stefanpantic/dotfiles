from student import Student

students = []
for i in range(3):
    print("name: ", end="")
    name = input()

    print("age: ", end="")
    age = int(input())

    students.append(Student(name, age))

file = open("students.csv", "w")
writer = csv.writer(file)
for student in students:
    writer.writerow((student.name, student.age))
