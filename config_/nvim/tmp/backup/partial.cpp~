#include <iostream>
#include <vector>

template <typename T, typename U>
void function(const T &arg1, const U &arg2)
{
	std::cout << "non" << std::endl;
}

template <int, typename U>
void function(const int &arg1, const U &arg2)
{
	std::cout << "yass" << std::endl;
}

int
main (void)
{
	function<float, std::vector<int>>(5.5, std::vector<int>{});
	function<int, double>(5, 4.4);
	return 0;
}
