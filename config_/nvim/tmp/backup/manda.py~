#!/bin/python

import re

line_pattern = re.compile(r"""
   (?P<date>\d{2}/\d{2}/\d{4})\s*
   (?P<time>\d{2}:\d{2}:\d{2}),\s*
   (?P<name>[\w ]+),\s*
   (?P<index>\d{1,4}/\d{1,4}),\s*
   (?P<group>\d+),
   (?P<optional>.*)?
""", re.VERBOSE)

file_str = 69
with open('./Daejana_drolja.csv', 'r') as f:
    file_str = f.read()

for match in line_pattern.finditer(file_str):
    print(match.group(0))

