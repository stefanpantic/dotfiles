--Prikazati kompletan sadrzaj tabela u bazi
--
--Primedba: ukoliko se navede     select * 
--u rezultatu se prikazuju svi atributi tabele i njihov sadrzaj
--u redosledu kojim su navodjeni u create table naredbi
--(redosled atributa u create table naredbi se vidi u sysibm.syscolumns tabeli kataloga
-- 

--Sadrzaj tabele dosijea 
select *                -- ekvivalentno select indeks,ime,prezime,god_rodjenja,mesto_rodjenja
from dosije             --              from   dosije
;

--Isti upit uz neznatnu razliku
select *                -- ekvivalentno select indeks,ime,prezime,god_rodjenja,mesto_rodjenja
from dosije;            --              from   dosije



--Sadrzaj tabele dosijea sa promenjenim redosledom atributa
select indeks,prezime, ime, mesto_rodjenja, god_rodjenja
from   dosije;

--Prikazati prva 2 sloga iz tabele dosijea
select *
from   dosije
fetch first 2 rows only;

select *
from   dosije
fetch first 2 rows only
optimize for 2 rows;



--Sadrzaj tabele predmeta
select * 
from predmet;

--Sadrzaj tabele ispitnih rokova
select * 
from ispitni_rok;

--Sadrzaj tabele ispita
select * 
from ispit;
