#!/bin/python

import sys

# expression: a|b*
current_state = 0
end_state = (0, 1, 2)
transition_map = {
        (0, 'a') : 1,
        (0, 'b') : 2,
        (2, 'b') : 2,
}

character = ''
while True:
    try:
        character = input()
        current_state = transition_map[(current_state, character)]
    except KeyError:
        print("\x1b[31minvalid expression...")
        sys.exit(1)
    except EOFError:
        break

if current_state in end_state:
    print("\x1b[34mvalid expression...")
else:
    print("\x1b[31minvalid expression...")
    sys.exit(1)

